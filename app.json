{
  "expo": {
    "name": "expo-starter-kit",
    "slug": "expo-starter-kit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash.png",
    "newArchEnabled": true,
    "scheme": "expo-starter-kit",
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#3988f7",
          "image": "./assets/splash.png",
          "dark": {
            "image": "./assets/splash.png",
            "backgroundColor": "#3988f7"
          }
        }
      ],
      [
        "expo-router",
        {
          "origin": "https://backend-eight-drab.vercel.app"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expocoder008.expostarterkit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.expocoder008.expostarterkit"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "server"
    }
  }
}
